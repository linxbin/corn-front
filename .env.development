# Delete console
VITE_DROP_CONSOLE=false

# Basic interface address SPA
VITE_GLOB_API_URL=http://localhost:8000/api/v1

# File upload addressï¼Œ optional
VITE_GLOB_UPLOAD_URL=/upload

# Interface prefix
